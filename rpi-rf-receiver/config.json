{
  "name": "RF Receiver",
  "version": "2.13",
  "slug": "rfreceiver",
  "description": "433 MHz RF Receiver",
  "url": "https://github.com/psallandre/hassio-addons",
  "startup": "application",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "homeassistant_api": true,
  "host_network": true,
  "devices": ["/dev/mem:/dev/mem:rw"],
  "privileged": ["SYS_RAWIO"],
  "gpio": "true",
  "apparmor": "false",
  "options": {
    "code": "/share/rpi-rf_receive.py",
    "requirements": ["rpi-rf"],
    "clean": true
  },
  "schema": {
    "code": "str",
    "requirements": ["str"],
    "clean": "bool?"
  },
  "map": ["share"]
}
