{
  "name": "RF Receiver",
  "version": "2.8",
  "slug": "rfreceiver",
  "description": "433 MHz RF Receiver",
  "url": "https://github.com/markvader/hassio-addons",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "hassio_api": true,
  "services": ["mqtt:need"],
  "devices": ["/dev/mem"],
  "full_access": true,
  "gpio": "true",
  "apparmor": "false",
  "map": [],
  "image": "markvader/hassio-rpi-rf-receiver-{arch}",
  "options": {
    "log_level": "info",
    "gpio": 27,
    "valid_codes": [],
    "mqtt": {},
    "mqtt_test": false
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "gpio": "int",
    "valid_codes": ["int"],
    "mqtt": {
      "host": "str?",
      "port": "str?",
      "user": "str?",
      "password": "str?"
    },
    "mqtt_test": "bool"
  }
}
